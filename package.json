{
  "name": "duecss",
  "type": "module",
  "version": "0.31.3",
  "packageManager": "pnpm@8.12.1",
  "description": "Minimal CSS framework built upon & extended by UnoCSS",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/duecss#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/duecss.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/duecss/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": "./styles/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"styles/**/*\" --write",
    "release": "bumpp --commit --push --tag"
  },
  "peerDependencies": {
    "unocss": "*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.6",
    "@unocss/core": "^0.58.0",
    "@unocss/preset-mini": "^0.58.0",
    "@unocss/preset-wind": "^0.58.0",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.58.0"
  }
}
