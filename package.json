{
  "name": "duecss",
  "version": "0.24.3",
  "description": "Minimal CSS framework built upon & extended by UnoCSS",
  "keywords": [
    "unocss",
    "unocss-preset"
  ],
  "homepage": "https://github.com/johannschopplich/duecss#readme",
  "bugs": {
    "url": "https://github.com/johannschopplich/duecss/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/duecss.git"
  },
  "license": "MIT",
  "author": {
    "name": "Johann Schopplich",
    "email": "pkg@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./styles/*"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "css": "sass styles/base/all.scss styles/base.css --style=expanded --no-source-map",
    "build": "pnpm run css && unbuild",
    "stub": "unbuild --stub",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "release": "bumpp package.json --commit --push --tag"
  },
  "dependencies": {
    "@unocss/core": "^0.28.0",
    "@unocss/preset-uno": "^0.28.0"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "typescript": "^4.6.2",
    "unbuild": "^0.7.0"
  }
}
