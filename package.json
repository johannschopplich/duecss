{
  "name": "duecss",
  "version": "0.15.3",
  "description": "Minimal CSS framework built upon & extended by UnoCSS",
  "keywords": [
    "unocss",
    "unocss-preset"
  ],
  "homepage": "https://github.com/johannschopplich/duecss#readme",
  "bugs": {
    "url": "https://github.com/johannschopplich/duecss/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/duecss.git"
  },
  "license": "MIT",
  "author": {
    "name": "Johann Schopplich",
    "email": "pkg@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./theme": {
      "require": "./dist/theme.js",
      "import": "./dist/theme.mjs",
      "types": "./dist/theme.d.ts"
    },
    "./*": "./styles/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "dev": "tsup --watch src",
    "css": "sass styles/base/all.scss styles/base.css --style=expanded --no-source-map",
    "build": "tsup && pnpm run css",
    "release": "bumpp package.json --commit --push --tag"
  },
  "devDependencies": {
    "@unocss/preset-uno": "^0.12.15",
    "bumpp": "^7.1.1",
    "prettier": "^2.5.0",
    "sass": "^1.44.0",
    "tsup": "^5.10.0",
    "typescript": "^4.5.2",
    "unocss": "^0.12.15"
  }
}
