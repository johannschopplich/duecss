{
  "name": "duecss",
  "version": "0.19.4",
  "description": "Minimal CSS framework built upon & extended by UnoCSS",
  "keywords": [
    "unocss",
    "unocss-preset"
  ],
  "homepage": "https://github.com/johannschopplich/duecss#readme",
  "bugs": {
    "url": "https://github.com/johannschopplich/duecss/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/duecss.git"
  },
  "license": "MIT",
  "author": {
    "name": "Johann Schopplich",
    "email": "pkg@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./styles/*"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "css": "sass styles/base/all.scss styles/base.css --style=expanded --no-source-map",
    "build": "unbuild && pnpm run css",
    "stub": "unbuild --stub",
    "release": "bumpp package.json --commit --push --tag"
  },
  "dependencies": {
    "@unocss/core": "^0.26.2",
    "@unocss/preset-uno": "^0.26.2"
  },
  "devDependencies": {
    "bumpp": "^7.1.1",
    "prettier": "^2.5.1",
    "sass": "^1.49.8",
    "typescript": "^4.5.5",
    "unbuild": "^0.6.9"
  }
}
