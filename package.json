{
  "name": "duecss",
  "type": "module",
  "version": "0.28.0",
  "packageManager": "pnpm@8.1.0",
  "description": "Minimal CSS framework built upon & extended by UnoCSS",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/duecss#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/duecss.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/duecss/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "require": "./dist/client.cjs",
      "import": "./dist/client.mjs"
    },
    "./*": "./styles/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag"
  },
  "peerDependencies": {
    "unocss": "*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.2",
    "@unocss/core": "^0.50.6",
    "@unocss/preset-mini": "^0.50.6",
    "@unocss/preset-wind": "^0.50.6",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "typescript": "^5.0.3",
    "unbuild": "^1.2.0",
    "unocss": "^0.50.6"
  }
}
